			"Option": {
				"Text": "#FFFFFF",
				"Hovering": {
					"Background": "#303030",
					"Text": "#FFFFFF"
				}
			},
			"Divider": "#282828"
		},
		"control": {
			"Buttons": {
				"Fill": "#FFFFFF",
				"Hovering": {
					"Background": "#353553"
				}
			},
			"Close": {
				"Button": {
					"Hovering": {
						"Background": "#E81123",
						"Fill": "white"
					},
					"Active": {
						"Background": "#F1707B",
						"Fill": "#161b22"
					}
				}
			}
		},
		"window": {
			"Background": "#161b22",
			"Icons": {
				"Fill": "rgb(40,40,40)"
			},
			"Border": "#404040"
		},
		"contextmenu": {
			"Background": "#161b22",
			"Button": {
				"Background": "#202020",
				"Text": "#FFFFFF",
				"Hovering": {
					"Background": "#303030",
					"Text": "#FFFFFF"
				}
			},
			"Divider": "#282828"
		},
		"mainpanel": {
			"Background": "#161b22"
		},
		"tabsbar": {
			"Background": "#282828"
