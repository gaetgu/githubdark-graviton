					"Hovering": {
						"Indicator": "#FFFFFF"
					}
				}
			}
		},
		"editor": {
			"Dialog": {
				"Background": "#161b22",
				"Text": "#FFFFFF"
			},
			"Tooltip": {
				"Background": "#21863b",
				"Text": "#1E1E3F"
			}
		},
		"statusbar": {
			"Background": "#161b22",
			"Border": "#303030",
			"Item": {
				"Background": "transparent",
				"Hovering": {
					"Background": "#404040",
					"Text": "#fff"
				},
				"Important": {
					"Background": "#1E1E3F",
					"Text": "#fff"
				},
				"Text": "#fff",
				"Icon": {
					"Background": "#fff"
				}
			},
			"Git": {
				"Item": {
					"Text": "#A599E9",
					"Modified": {
						"Text": "#A599E9"
					}
				}
			}
		},
		"input": {
			"Background": "#161b22",
			"Text": "#21863b",
			"Border": "#404040"
		},
		"splash": {
			"Screen": {
				"Text": "#21863b",
				"Background": "#2D2B55"
			}
		},
		"command": {
			"Prompt": {
				"Input": {
					"Background": "#161b22",
					"Text": "#1E1E3F"
				},
				"Background": "#2D2B55",
				"Option": {
					"Background": "#21863b",
					"Active": {
						"Background": "#161b22",
						"Text": "white"
					},
					"Text": "#161b22"
				}
			}
		},
		"notification": {
			"Background": "#2D2B55",
			"Title": {
				"Text": "#161b22"
			},
			"Content": {
				"Text": "#161b22"
			},
			"Button": {
				"Background": "#21863b"
			}
		},
		"panel": {
			"Border": "#404040"
		},
		"switch": {
			"Background": "white",
			"Indicator": {
				"Activated": {
					"Background": "orange"
				},
				"Desactivated": {
					"Background": "rgba(150,150,150,0.8)"
				}
			}
		},
		"loader": {
			"Background": "#FFFFFF"
		},
		"sidebar": {
			"Icons": {
				"Fill": "#A599E9",
				"Active": {
					"Fill": "#FFFFFF"
				},
				"Hovering": {
					"Fill": "#FFFFFF"
				}
			},
			"Icon": {
				"Background": "transparent",
				"Hovering": {
					"Background": "transparent"
				},
				"Active": {
					"Background": "#161b22"
				}
			},
			"Background": "#161b22"
		}
	}
