{
	"name": "GitHub Dark",
	"id": "github-dark",
	"author": "Gabriel Guti√©rrez",
	"version": "1.0.0",
	"description": "Unofficial Port of the GitHub Dark Theme for Graviton2",
	"textTheme": "ghdark",
	"fileTheme": "ghdark.css",
	"type": "theme",
	"colorsScheme": {
		"terminal": {
			"Background": "#1E1E3F",
			"Foreground": "#FFFFFF",
			"Selection": "#FAD000",
			"Cursor": "#FAD000"
		},
		"accent": {
			"Color": "#21863b"
		},
		"text": {
			"Color": "#ffffff",
			"Focused": {
				"Background": "#FFFFFF"
			}
		},
		"body": {
			"Background": "#161b22"
		},
		"titlebar": {
			"Background": "#161b22"
		},
		"menu": {
			"Background": "#161b22",
			"Button": {
				"Background": "#202020",
				"Hovering": {
					"Background": "#303030",
					"Text": "#FFFFFF"
				},
				"Text": "#FFFFFF"
