						"Background": "#222244"
					}
				}
			},
			"Icon": {
				"Fill": "#FFFFFF",
				"Hovering": {
					"Fill": "#FFFFFF"
				}
			}
		},
		"scrollbar": {
			"Background": "#101010",
			"Hovering": {
				"Background": "#282828"
			}
		},
		"sidemenu": {
			"Background": "#232323",
			"Button": {
				"Background": "transparent",
				"Hovering": {
					"Background": "#161b22"
				},
				"Active": {
					"Background": "#21863b",
					"Text": "white"
				},
				"Text": "white"
			},
			"Searcher": {
				"Background": "white",
				"Text": "#161b22",
				"Border": "#CFCFCF"
			}
		},
		"explorer": {
			"Item": {
				"Text": "#fff",
				"Hovering": {
					"Background": "#53a5fc"
				},
				"Selected": {
					"Background": "#0071fc"
				},
				"Arrow": {
					"Background": "#60a6fc"
				},
				"Git": {
					"Modified": {
						"Text": "#21863b",
						"Indicator": "#21863b"
					},
					"Not": {
						"Added": {
							"Text": "#00FF009A",
							"Indicator": "#00FF009A"
						}
					},
					"Indicator": {
						"Text": "#1E1E3F"
					}
				}
			}
		},
		"card": {
			"Background": "#232323"
